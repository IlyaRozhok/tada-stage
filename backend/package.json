{
  "name": "rental-platform-backend",
  "version": "1.0.0",
  "description": "NestJS backend for rental platform",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "nest start --watch",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -d src/database/data-source.ts",
    "typeorm:migration:run": "npm run typeorm -- migration:run -d src/database/data-source.ts",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert -d src/database/data-source.ts",
    "typeorm:dev": "typeorm-ts-node-commonjs -d src/database/data-source.ts",
    "typeorm:prod": "typeorm -d dist/database/data-source.js",
    "migration:run:dev": "npm run typeorm:dev migration:run",
    "migration:run:prod": "npm run typeorm:prod migration:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.4.19",
    "@nestjs/swagger": "^7.1.11",
    "@nestjs/typeorm": "^10.0.0",
    "@types/cors": "^2.8.19",
    "@types/multer": "^1.4.13",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "multer": "^2.0.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^9.0.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
